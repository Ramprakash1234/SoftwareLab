<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
<link rel="stylesheet"href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
<link href="https://fonts.googleapis.com/css2?family=Lobster&family=Lobster+Two&display=swap" rel="stylesheet">
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
<style>
	body{
		font-family:Lobster, cursive;	
	}
	.container{
		margin:15px auto;
	}
	.singleemployee{
		border:1px solid #eee;
		box-shadow:0px 0px 2px blue;
		padding:15px 40px;
		border-radius:15px;
		margin:15px auto;
	}
</style>
<div class="container">
	<div class="jumbotron">
		<h3>Employee List</h3> 
		<a type="button" class="btn btn-outline-info" href="/addEmployee">Add</a>
	</div>
</div>
<%if(employees.length>0){%>
<div class="container">
	<%for (var employeeNo=0;employeeNo<employees.length;employeeNo++){%>
		<div class="singleemployee">
			<h1>Name: <%=employees[employeeNo].eName%></h1>
			<h3>E-Mail: <%=employees[employeeNo].eMail%></h3>
			<p>Address: <%=employees[employeeNo].eAddress%></p>
			<p>Phone: <%=employees[employeeNo].ePhone%></p>
			<p>Salary: <%=employees[employeeNo].eSalary%></p>
		</div>
	<%}%>
</div>
<%}else{%>
<div class="container">
	<h4 style="text-align:center;">No Employee details available</h4>
</div>
<%}%>
	